{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "./index.html"
      },
      {
        "source": "/subpage/**", 
        "destination": "/subpage/index.html"
      }
    ]
  }
}
